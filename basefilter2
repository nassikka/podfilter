// FILTER LEVELS
// -------------------------------------------------------------------------
FilterLevel[1]: Normal 
FilterLevel[2]: Strict (Engame)
Option[DefaultFilterLevel]: Normal
Option[DefaultNotifyLevel]: LOW

// -------------------------------------------------------------------------
// TEXT MACROS
// -------------------------------------------------------------------------
TextMacro[ALLRES]: %WHITE%%RES%%YELLOW%a%RED%l%BLUE%l%YELLOW%r%RED%e%BLUE%s
TextMacro[RUNESEND]: %PURPLE%OO%WHITE%OO%PURPLE%OO
TextMacro[RUNESSTART]: %PURPLE%OO%WHITE%OO%PURPLE%OO
TextMacro[MAP]: %PURPLE%O%LIGHT_GRAY% [L%ILVL%]
TextMacro[STARS]: %PURPLE%O
TextMacro[STARS1]: %R%O%WHITE%O%RED%O
TextMacro[STARS2]: %RED%O%WHITE%OO%RED%O
ItemStyle[HR]: MapIcon = 2, MapIconColor = 0, BorderColor = 155, BorderSize = 2, BackgroundColor = 15, NotificationPriority = HIGH, NotificationColor = ITEM, NotificationSoundPriority = MEDIUM, NotificationSound = "@global\sfx\cursor\hostile"
ItemStyle[MID]: MapIcon = 3, MapIconColor = 3, BorderColor = 10, BorderSize = 2, BackgroundColor = 15, NotificationPriority = HIGH, NotificationColor = ITEM, NotificationSoundPriority = MEDIUM, NotificationSound = "@global\sfx\cursor\levelup"
ItemStyle[BASIC]: BorderColor = 4, BorderSize = 2, BackgroundColor = 172
ItemStyle[BASICLOW]: NotificationPriority = LOW, NotificationColor = ITEM, BorderColor = 4, BorderSize = 2, BackgroundColor = 15
ItemStyle[BASICMED]: NotificationPriority = MEDIUM, NotificationColor = ITEM, BorderColor = 4, BorderSize = 2, BackgroundColor = 15
