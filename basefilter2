ItemStyle[BASIC1]: BorderColor = 4, BorderSize = 2, BackgroundColor = 138
ItemStyle[BASICNOT]: NotificationPriority = HIGH, NotificationColor = ITEM, BorderColor = 4, BorderSize = 2, BackgroundColor = 138
